{
  "expo": {
    "name": "Colour Memory",
    "slug": "ColourMemory",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
